{
  "containers": {
    "containerDefinitions": [
      {
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/tomcat-container-logs",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "ecs"
          }
        },
        "portMappings": [
          {
            "hostPort": 8080,
            "protocol": "tcp",
            "containerPort": 8080
          }
        ],
        "cpu": 512,
        "memory": 256,
        "image": "tomcat",
        "essential": true,
        "name": "tomcat-webserver"
      }
    ],
    "requiresCompatibilities": [
      "EC2"
    ],
    "networkConfiguration": {
      "awsvpcConfiguration": {
        "subnets": [
          "subnet-035fa2a23fbf54e98"
        ],
        "securityGroups": [
          "sg-0ce7e066409332f31"
        ],
        "assignPublicIp": "DISABLED"
      }
    },
    "cluster": "boto-test",
    "taskDefinition": "tomcat-webserver",
    "memory": "256",
    "family": "tomcat-webserver",
    "networkMode": "awsvpc",
    "cpu": "512",
    "region": "us-east-1"
  }
}

